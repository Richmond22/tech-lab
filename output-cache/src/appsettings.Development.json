{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedisOptions": {
    "ConnectionString": "redis:6379",
    "InstanceName": "OutputCache:",
    "ConnectRetry": 3,
    "ConnectTimeout": 5000,
    "AbortOnConnectFail": false,
    "AllowAdmin": false
  },
  "DatabaseOptions": {
    "ConnectionString": "Host=db;Database=outputcache;Username=postgres;Password=postgres",
    "MaxRetryCount": 3,
    "CommandTimeout": 30,
    "EnableDetailedErrors": false,
    "EnableSensitiveDataLogging": false
  }
}
